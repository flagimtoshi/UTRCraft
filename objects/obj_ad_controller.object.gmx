        case "banner_load":
            if (async_load[? "loaded"] == 1) {
                GoogleMobileAds_MoveBanner(display_get_gui_width()/2 - GoogleMobileAds_BannerGetWidth()/2, display_get_gui_height() - GoogleMobileAds_BannerGetHeight());
