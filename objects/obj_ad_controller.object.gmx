            }
        break;
        case "interstitial_load":
            if (async_load[? "loaded"] == 1) {
                interstitial_loaded = true;
                //show_message_async("Interstitial loaded");
            }
        break;
        case "rewardedvideo_adloaded":
            rewarded_loaded = true;   
            //show_message_async("Rewarded loaded");     
